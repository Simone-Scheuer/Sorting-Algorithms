def insertion_sort(arr):
  for i in range (1,len(arr)):
    key = arr[i]
    j = i - 1
    while j >= 0 and key < arr[j] :
      j -= 1
    arr[j + 1] = key
  return arr


my_list = [22, 11, 99, 88, 9, 7, 42]
print("Original list:", my_list)
sorted_list = insertion_sort(my_list)
print("Sorted list:", sorted_list)
  
